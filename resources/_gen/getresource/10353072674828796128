            <!--bookshop-live name(structures/header/burger-menu)-->


<button class="c-burger-menu"
x-on:click="burgerOpen = ! burgerOpen"
x-data="{
        toggleScrollLock(isBurgerMenuOpen){
                if(isBurgerMenuOpen === true){
                        targetElement = document.querySelector('#menu-dropdown');
                        disableScroll(targetElement, {
                            allowTouchMove: el => {
                                while (el && el !== document.body) {
                                    if (el.getAttribute('body-scroll-lock-ignore') !== null) {
                                        return true;
                                    }
                                    el = el.parentElement;
                                }
                            }
                        });    
                }else {
                        targetElement = document.querySelector('#menu-dropdown');
                        enableScroll(targetElement);
                }
                       
        }
}"
x-init="
        $watch('burgerOpen', value => toggleScrollLock(value));
">
        <div class="c-burger-menu__burger" 
        x-bind:class="burgerOpen ? 'c-burger-menu__burger--open' : 'c-burger-menu__burger'"
        id="burger-menu"
        ></div>
</button>

<!--bookshop-live end-->
        </div>
        <div class="c-nav-bar__mobile__headings" x-show="burgerOpen" x-cloak>
            <!--bookshop-live name(structures/header/mobile-nav-heading-item-group)-->


