<script async>
window.addEventListener("load", function(event) {
    const linksSidebar = document.querySelectorAll(".nav-wrapper .sub-link-wrapper .sub-link-nav");
	const linksSidebarActives = document.querySelectorAll(".nav-wrapper .sub-link-wrapper .sub-link-nav");
	const linksMenuSidebar = document.querySelectorAll(".navbar-runbook-mobile .nav-dropdown-link-2");
    const linksMenuPrincipal = document.querySelectorAll(".navigation__runbook .link-nav-header .link-nav-open");
   
    for (const link of linksSidebar) {
        link.addEventListener("click", clickToScroll);
    }
  
    for (const linkRunbook of linksMenuSidebar) {
         linkRunbook.addEventListener("click", clickToScroll);
    }
    for (const linkPrincipal of linksMenuPrincipal) {
         linkPrincipal.addEventListener("click", clickToScroll);
    }

    function clickToScroll(e) {
        e.stopPropagation();
        e.preventDefault();
        var parentPathname = window.location.pathname
        var parentLink = this.dataset.parent;
        const href = this.dataset.ref;
        console.log(parentPathname,parentLink, href)

        if( parentPathname !=  parentLink){
            console.log('is othe url')
            window.location.hash = href;
            window.location.pathname = parentLink;
        } else {

            for (const linkRemove of linksSidebarActives) {
                linkRemove.classList.remove('active');
            }
            this.classList.add('active');
            const href = this.dataset.ref;
            const offsetTop = document.querySelector('#'+href).offsetTop-15;

            console.log("1 "+href, offsetTop)
          	
            var mobileMenu = document.querySelectorAll('.menu-button-3')
			menu_mobile.click()

            scroll({
                top: offsetTop,
                behavior: "smooth"
            }); 
         }        
    }
  	function clickToScrollR(e) {
        e.stopPropagation();
        e.preventDefault();
        var parentPathname = window.location.pathname
