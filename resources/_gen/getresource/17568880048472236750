    var redirectPaths = ["clients", "query-language", "mutations", "deploy", "enterprise-features", "howto", "design-concepts"];
    var pathSplit = document.URL.split("/");
    
    
    
    
    if (redirectPaths.includes(pathSplit[pathSplit.length - 2])) {
      var urlSplit = pathSplit[pathSplit.length - 1].split("#");
      var origPath = "/" + pathSplit[pathSplit.length - 2] + "/#" + urlSplit[1];
      if (origPath in subPaths) {
        loc = "https:\/\/dgraph.io\/docs\/" + subPaths[origPath];
        window.location.replace(loc);
      } else if (urlSplit[1]) { 
        loc = "https:\/\/dgraph.io\/docs\/" + pathSplit.slice(4, pathSplit.length-1).join("/") + "/#" + urlSplit[1];
        window.location.replace(loc);
      }
    }
