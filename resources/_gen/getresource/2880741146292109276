    }  
 	var headerIconsMenu = document.querySelectorAll('.link-nav-header .nav-dropdown-icon-desktop')
    for (const headerMenu of headerIconsMenu) {
        
        headerMenu.addEventListener("click", function(element){
            element.preventDefault()
            element.stopPropagation()
            if(element.target.parentElement.parentElement.classList.contains('open')){
                element.target.parentElement.parentElement.classList.remove('open')
            } else {
                closeHeaders()
                element.target.parentElement.parentElement.classList.toggle('open')
            }
        })
    }

    function closeHeaders() {
        var headerIconsMenu = document.querySelectorAll('.navigation__runbook .link-nav-header')
        for (const headerMenu of headerIconsMenu) {
            headerMenu.classList.remove('open')
        }
        var sublinks = document.querySelectorAll('.navigation__runbook .sub-link-wrapper')
        for (const sublink of sublinks) {
            sublink.style.display = 'none'
        }
        var sublinksCurrent = document.querySelectorAll('.navigation__runbook .w--current')
        for (const sublinkCurrent of sublinksCurrent) {
            sublinkCurrent.classList.remove('w--current')
        }
       
    }
  if(window.location.hash != ''){
    
    //menu runbook en mobile
        var hashLink = window.location.hash.substring(1)
        
        console.log("2 "+hashLink)
        if(window.location.pathname == '/runbook/linkerd-runbook'){
           if(window.innerWidth > 991){
             console.log("3 ");
             	setTimeout(function(){
                	document.querySelector('.link-nav[data-ref="'+hashLink+'"]').click()
            	},100)
               
            } else {
               console.log("4 ");
                var welcome = document.querySelector('#welcome').clientHeight
                var runbook_header = document.querySelector('.navbar-wrapper-2').clientHeight
                var header = document.querySelector('.menu-button-2').clientHeight
                var offset = welcome + runbook_header + header - 65
            }

            scroll({
                top: offset ,
                behavior: "smooth"
            });
        }  else {
    		console.log("5 ");
            setTimeout(function(){
                document.querySelector('.sub-link-nav[data-ref="'+hashLink+'"]').click()
            },100)
        }
    }

    var isFirstMenu  = document.querySelector('.nav-guide-runbook')
    if(isFirstMenu) {
        isFirstMenu.addEventListener('click', function(e){
            e.preventDefault()
            e.stopPropagation()
          console.log('----');
            // same url
            if(window.location.pathname == '/runbook/linkerd-runbook'){
              console.log('a'); 
              const href = this.dataset.ref;
              const offsetTop = document.querySelector('#'+href).offsetTop-15;
              console.log("1 "+href, offsetTop)
              scroll({
                  top: offsetTop,
                  behavior: "smooth"
              }); 
            } else {
              	console.log('b');
                          	
                var href = isFirstMenu.dataset.parent
                var hash = isFirstMenu.dataset.ref
                console.log(isFirstMenu)
                console.log(isFirstMenu.dataset)
                window.location.hash = hash
                window.location.pathname = href                
            }
        })
    }
  var isFirstMenuMob  = document.querySelector('.nav-guide-runbookmob')
    if(isFirstMenuMob) {
        isFirstMenuMob.addEventListener('click', function(e){
            e.preventDefault()
