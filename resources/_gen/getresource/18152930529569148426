      (function(mainBody) {
        const handler = function () {
          const is_mobile = window.innerWidth < 1280;
          const previously_focused_element_is_input = document.activeElement && document.activeElement.nodeName.toLowerCase() === 'input';

          if (is_mobile && previously_focused_element_is_input) return;

          mainBody.style.minHeight = window.innerHeight + "px";
        }
        handler();
        window.addEventListener("resize", handler);
        window.addEventListener("orientationchange", handler);
      })(document.getElementById("mainBody"));
