    active: 'bg-red-6 text-white hover:bg-red-5 focus:ring-red-7 active:ring-red-9',
    inactive: 'bg-s-2 text-s-9 hover:bg-s-3 focus:ring-s-7 active:ring-s-9',
    buildClass(cs, val) {
      return Object.fromEntries(cs.split(/ /).map(c=>[c, val]));
    },
    init() {
      this.activeClasses = {
        ...this.buildClass(this.active, true),
        ...this.buildClass(this.inactive, false),
      }
      this.inactiveClasses = {
        ...this.buildClass(this.active, false),
        ...this.buildClass(this.inactive, true),
      }
    },
    amount: '48',
    freq: 'm',
    showOther: false,
    btnClass(cond) {
      return cond? this.activeClasses : this.inactiveClasses;
    },
    setAmt(freq, amount) {
      this.freq = freq;
      this.amount = amount;
      this.showOther = false;
    },
    get donateURL() {
      let campaign = this.$el.
        closest('[data-donate-campaign]')?.dataset.donateCampaign;
      let theme =  this.$el.
        closest('[data-donate-theme]')?.dataset.donateTheme;
      let period = {
        o: 'once',
        m: 'monthly',
        y: 'yearly',
      }[this.freq];
      return `https://checkout.fundjournalism.org/memberform?org_id=spotlightpa&campaign=${campaign}&theme=${theme}&amount=${this.amount}&installmentPeriod=${period}&utm_source=spotlightpa.org&utm_medium=elections&utm_campaign=donate-prefill`;
    }
  }"><div class="flex flex-wrap items-center"><h2 class="flex-1 font-sans text-3xl font-black leading-none tracking-wide text-white sm:text-4xl md:text-5xl">Support Spotlight PA's vital investigative & public service journalism.</h2><p class="my-4 font-sans text-lg text-white">Our work is only possible with the support of our readers.</p><div class="mt-6 flex flex-wrap justify-start gap-2"><button type=button class="mr-4 flex-[1_1_30%] sm:grow-0 outline-none min-w-[180px] inline-block rounded py-3 px-6 text-white font-sans font-black text-lg tracking-wide ring ring-transparent bg-s-2 text-s-9 hover:bg-s-3 focus:ring-s-7 active:ring-s-9" :class="btnClass(amount === '10')" @click="amount = '10'" x-show="freq === 'm'">
